 {
  "compilerOptions": {
    "module": "ESNext",                                
    "rootDir": "./src",
    "outDir": "./dist",  
    "moduleResolution": "bundler", 
     "target": "ES2023",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
        "ignoreDeprecations": "6.0"

  },
  "include": ["src"],
  "exclude": ["node_module","dist"]
}